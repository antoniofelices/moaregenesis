{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": false,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"link": true,
			"palette": [
				{
					"slug": "background",
					"color": "#f1f1f1",
					"name": "Background"
				},
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "primary",
					"color": "#ff0000",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#00ff00",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#0000ff",
					"name": "Tertiary"
				},
				{
					"slug": "gray",
					"color": "#535353",
					"name": "Gray"
				},
				{
					"slug": "gray-dark",
					"color": "#2d2d2d",
					"name": "Gray dark"
				},
				{
					"slug": "gray-lighter",
					"color": "#c6c6c6",
					"name": "Gray lighter"
				}
			]
		},
		"custom": {
			"typography": {
				"adaptiveRatio": "0.8",
				"fontWeight": {
					"normal": "400",
					"bold": "700"
				},
				"lineHeight": {
					"small": "1.1",
					"normal": "1.5",
					"large": "1.7"
				},
				"rootSize": "1rem",
				"scale": "1.125"

			},
			"magicNumber": "1.5rem",
			"gutter": "1.5rem"
		},
		"layout": {
			"contentSize": "50vw",
			"wideSize": "60vw"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": [
				"px",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"customFontSize": false,
			"fontStyle": false,
			"fontWeight": true,
			"letterSpacing": false,
			"lineHeight": false,
			"textDecoration": false,
			"textTransform": true,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Sans serif\", sans-serif",
					"name": "Sans serif",
					"slug": "sans-serif"
				},
				{
					"fontFamily": "\"Sans serif 2\", sans-serif",
					"name": "Sans serif 2",
					"slug": "sans-serif-2"
				},
				{
					"fontFamily": "\"Serif\", serif",
					"name": "Serif",
					"slug": "serif"
				}
			],
			"fontSizes": [
				{
					"name": "Extra small",
					"size": "calc(var(--wp--preset--font-size--small) / var(--wp--custom--typography--scale))",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": "calc(var(--wp--preset--font-size--medium) / var(--wp--custom--typography--scale))",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "var(--wp--custom--typography--root-size)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "calc(var(--wp--preset--font-size--medium) * var(--wp--custom--typography--scale))",
					"slug": "large"
				},
				{
					"name": "X large",
					"size": "clamp(1.602rem, 2.027rem, 5vw)",
					"slug": "x-large"
				},
				{
					"name": "Gigantic",
					"size": "clamp(2.027rem, 4.11rem, 10vw)",
					"slug": "gigantic"
				}
			]
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"textTransform": false,
					"fontWeight": false
				}
			},
			"core/button": {
				"border": {
					"radius": false
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--black)"
		},
		"spacing": {
			"blockGap": "var(--wp--custom--magic-number)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--normal)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--gigantic)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h2": {
				"typography": {
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h3": {
				"typography": {
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h4": {
				"typography": {
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			}
		},
		"blocks": {
			"core/button": {
				"border": {
					"color": "var(--wp--preset--color--black)",
					"radius": "0px",
					"style": "solid",
					"width": "1px"
				},
				"color": {
					"background": "transparent",
					"text": "var(--wp--preset--color--black)"
				}
			}
		}
	}
}