{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": false,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"link": true,
			"palette": [
				{
					"slug": "background",
					"color": "#f1f1f1",
					"name": "Background"
				},
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "primary",
					"color": "#ff0000",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#00ff00",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#0000ff",
					"name": "Tertiary"
				},
				{
					"slug": "quarter",
					"color": "#fec900",
					"name": "Quarter"
				},
				{
					"slug": "fifth",
					"color": "#ff8b70",
					"name": "Fifth"
				},
				{
					"slug": "gray",
					"color": "#535353",
					"name": "Gray"
				},
				{
					"slug": "gray-dark",
					"color": "#2d2d2d",
					"name": "Gray dark"
				},
				{
					"slug": "gray-darker",
					"color": "#1c1c1c",
					"name": "Gray darker"
				},
				{
					"slug": "gray-light",
					"color": "#bbb8bb",
					"name": "Gray light"
				},
				{
					"slug": "gray-lighter",
					"color": "#c6c6c6",
					"name": "Gray lighter"
				},
				{
					"slug": "gray-lightest",
					"color": "#edeced",
					"name": "Gray lightest"
				},
				{
					"slug": "text-color",
					"color": "#000000",
					"name": "text-color"
				},
				{
					"slug": "link-color",
					"color": "#000000",
					"name": "link-color"
				}
			]
		},
		"custom": {
			"typography": {
				"adaptiveRatio": "0.8",
				"fontWeight": {
					"normal": "400",
					"bold": "700"
				},
				"lineHeight": {
					"extra-small": "1",
					"small": "1.1",
					"medium": "1.35",
					"normal": "1.5",
					"large": "1.7"
				},
				"rootSize": "1rem",
				"scale": "1.125"
			},
			"magicNumber": "1rem",
			"gutter": "1rem"
		},
		"layout": {
			"contentSize": "650px",
			"wideSize": "60vw"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1rem, 2vw, 2rem)",
					"slug": "30",
					"name": "1"
				},
				{
					"size": "clamp(1rem, 3vw, 3rem)",
					"slug": "40",
					"name": "2"
				},
				{
					"size": "clamp(1rem, 4vw, 4rem)",
					"slug": "50",
					"name": "3"
				},
				{
					"size": "clamp(1rem, 5vw, 5rem)",
					"slug": "60",
					"name": "4"
				},
				{
					"size": "clamp(1rem, 6vw, 6rem)",
					"slug": "70",
					"name": "5"
				}
			],
			"units": [
				"px",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"customFontSize": false,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"fluid": true,
			"textDecoration": true,
			"textTransform": true,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/dm-sans/DMSans-Regular.woff2"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/dm-sans/DMSans-Regular-Italic.woff2"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/dm-sans/DMSans-Bold.woff2"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/dm-sans/DMSans-Bold-Italic.woff2"
							]
						}
					],
					"fontFamily": "\"DM Sans\", sans-serif",
					"name": "DM Sans",
					"slug": "dm-sans"
				},
				{
					"fontFamily": "\"Sans serif\", sans-serif",
					"name": "Sans serif",
					"slug": "sans-serif"
				},
				{
					"fontFamily": "\"Serif\", serif",
					"name": "Serif",
					"slug": "serif"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Extra small",
					"size": ".79rem",
					"slug": "extra-small"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": ".889rem",
					"slug": "small"
				},
				{
					"fluid": false,
					"name": "Base",
					"size": "1rem",
					"slug": "base"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1.266rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.266rem",
						"max": "1.802rem"
					},
					"name": "Large",
					"size": "1.802rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.602rem",
						"max": "2.281rem"
					},
					"name": "X Large",
					"size": "2.281rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "3.653rem",
						"max": "4.624rem"
					},
					"name": "XX Large",
					"size": "4.11rem",
					"slug": "xx-large"
				}
			]
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": false
				}
			}
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/quote": {
				"border": {
					"left": {
						"color": "var(--wp--preset--color--black)",
						"style": "solid",
						"width": "2px"
					}
				}
			},
			"core/separator": {
				"border": {
					"bottom": {
						"width": "0"
					},
					"left": {
						"width": "0"
					},
					"top": {
						"width": "1px"
					},
					"right": {
						"width": "0"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--text-color)"
		},
		"elements": {
			"button": {
				"border": {
					"color": "var(--wp--preset--color--primary)",
					"radius": "0",
					"style": "solid",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "1rem"
				},
				":hover": {
					"border": {
						"color": "var(--wp--preset--color--gray-darker)"
					},
					"color": {
						"background": "var(--wp--preset--color--gray-darker)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":focus": {
					"border": {
						"color": "var(--wp--preset--color--gray-darker)"
					},
					"color": {
						"background": "var(--wp--preset--color--gray-darker)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":active": {
					"border": {
						"color": "var(--wp--preset--color--gray-darker)"
					},
					"color": {
						"background": "var(--wp--preset--color--gray-darker)",
						"text": "var(--wp--preset--color--white)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "clamp(2.027rem, 4.5vw, 3.247rem)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(1.602rem, 3.75vw, 2.566rem)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "clamp(1.602rem, 3vw, 1.802rem)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "clamp(1.602rem, 3vw, 1.802rem)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "clamp(1.602rem, 3vw, 1.802rem)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "clamp(1.602rem, 3vw, 1.802rem)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)",
					"fontWeight": "700"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					},
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--magic-number)",
			"padding": {
				"top": "var(--wp--preset--spacing--30)",
				"right": "var(--wp--preset--spacing--30)",
				"bottom": "var(--wp--preset--spacing--30)",
				"left": "var(--wp--preset--spacing--30)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--normal)"
		}
	}
}
