{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": false,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"link": true,
			"palette": [
				{
					"slug": "background",
					"color": "#f1f1f1",
					"name": "Background"
				},
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "primary",
					"color": "#ff0000",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#00ff00",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#0000ff",
					"name": "Tertiary"
				},
				{
					"slug": "gray",
					"color": "#535353",
					"name": "Gray"
				},
				{
					"slug": "gray-dark",
					"color": "#2d2d2d",
					"name": "Gray dark"
				},
				{
					"slug": "gray-lighter",
					"color": "#c6c6c6",
					"name": "Gray lighter"
				}
			]
		},
		"custom": {
			"typography": {
				"adaptiveRatio": "0.8",
				"fontWeight": {
					"normal": "400",
					"bold": "700"
				},
				"lineHeight": {
					"small": "1.1",
					"normal": "1.5",
					"large": "1.7"
				},
				"rootSize": "1rem",
				"scale": "1.125"
			},
			"magicNumber": "1.5rem",
			"gutter": "1.5rem"
		},
		"layout": {
			"contentSize": "40vw",
			"wideSize": "60vw"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": [
				"px",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"customFontSize": false,
			"fontStyle": false,
			"fontWeight": true,
			"letterSpacing": false,
			"lineHeight": true,
			"textDecoration": false,
			"textTransform": true,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/dm-sans/DMSans-Regular.woff2"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/dm-sans/DMSans-Regular-Italic.woff2"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/dm-sans/DMSans-Bold.woff2"
							]
						},
						{
							"fontFamily": "DM Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/dm-sans/DMSans-Bold-Italic.woff2"
							]
						}
					],
					"fontFamily": "\"DM Sans\", sans-serif",
					"name": "DM Sans",
					"slug": "dm-sans"
				},
				{
					"fontFamily": "\"Sans serif\", sans-serif",
					"name": "Sans serif",
					"slug": "sans-serif"
				},
				{
					"fontFamily": "\"Serif\", serif",
					"name": "Serif",
					"slug": "serif"
				}
			],
			"fontSizes": [
				{
					"name": "Extra small",
					"size": ".79rem",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": ".889rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "var(--wp--custom--typography--root-size)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.266rem, 3vw, 1.602rem)",
					"slug": "large"
				},
				{
					"name": "X large",
					"size": "clamp(1.602rem, 3.75vw, 2.027rem)",
					"slug": "x-large"
				},
				{
					"name": "Gigantic",
					"size": "clamp(2.027rem, 4.5vw, 4.11rem)",
					"slug": "gigantic"
				}
			]
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"textTransform": false,
					"fontWeight": false
				}
			},
			"core/button": {
				"border": {
					"radius": false
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--black)"
		},
		"spacing": {
			"blockGap": "var(--wp--custom--magic-number)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--normal)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					},
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				"typography": {
					"textDecoration": "none"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "clamp(2.027rem, 4.5vw, 3.247rem)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(1.602rem, 3.75vw, 2.566rem)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "clamp(1.266rem, 3vw, 2.281rem)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "clamp(1.125rem, 2.25vw, 2.027rem)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "clamp(1rem, 1.5vw, 1.802rem)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-sans)",
					"fontWeight": "700"
				}
			}
		},
		"blocks": {
			"core/button": {
				"border": {
					"color": "var(--wp--preset--color--black)",
					"radius": "0px",
					"style": "solid",
					"width": "1px"
				},
				"color": {
					"background": "transparent",
					"text": "var(--wp--preset--color--black)"
				}
			}
		}
	}
}